<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitnessapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeThree">
        <activity
            android:name=".ui.add_results.AddingResults"
            android:theme="@style/AppThemeTwo" />
        <activity
            android:name=".ui.exercise_description.FlutterKicks"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".ui.exercise_description.Plank"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".ui.exercise_description.HeelTouch"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".ui.exercise_description.LegRaises"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".ui.exercise_description.ReverseCrunches"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".ui.exercise_description.MountainClimber"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".ui.exercise_description.HighStepping"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".ui.exercise_description.JumpingJacks"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".Register"
            android:theme="@style/AppThemeTwo" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Login"
            android:theme="@style/AppThemeTwo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>